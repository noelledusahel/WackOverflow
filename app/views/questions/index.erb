<div>
  <div class="container">
    <label>Feeling Wack?</label><br>
    <a href="/questions/new">Post New Wack Question</a>
  </div>
  <div class='questions-container'>
    <% @questions.each do |question| %>
    <div class='container' id="<%= question.id %>">
      <h3><a href="/questions/<%= question.id %>"><%= question.title %></a></h3>
      <p> by: <%= question.author.username %> </p>
    </div>
    <% end %>
  </div>
</div>
